<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.magellan2018.alice.app" ios-CFBundleVersion="1.3.3" version="1.3.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Magellan LARP 2018</name>
    <description>Magellan 2018 LARP Mobile Application.</description>
    <author email="info@magellan2018.ru" href="http://magellan2018.ru/">Saint-Petersburg Format</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/an-icon-36-ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/an-icon-48-mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/an-icon-72-hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/an-icon-96-xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/an-icon-144-xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/an-icon-192-xxxhdpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/and-ss15-320-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/and-ss16-480-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/and-ss17-720-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/and-ss16-960-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/and-ss17-1080-xxxhdpi.png" />
        <splash density="mdpi" src="resources/android/splash/and-land-ss15-320-mdpi.png" />
        <splash density="hdpi" src="resources/android/splash/and-land-ss16-480-hdpi.png" />
        <splash density="xhdpi" src="resources/android/splash/and-land-ss17-720-xhdpi.png" />
        <splash density="xxhdpi" src="resources/android/splash/and-land-ss16-960-xxhdpi.png" />
        <splash density="xxxhdpi" src="resources/android/splash/and-land-ss17-1080-xxxhdpi.png" />
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash height="480" src="resources/ios/splash/ss15-ios320.png" width="320" />
        <splash height="960" src="resources/ios/splash/ss15-ios640.png" width="640" />
        <splash height="1136" src="resources/ios/splash/ss17-ios640.png" width="640" />
        <splash height="1334" src="resources/ios/splash/ss17-ios750.png" width="750" />
        <splash height="2208" src="resources/ios/splash/ss17-ios1242.png" width="1242" />
        <icon height="180" src="resources/ios/icon/ios-icon-180.png" width="180" />
        <icon height="167" src="resources/ios/icon/ios-icon-167.png" width="167" />
        <icon height="152" src="resources/ios/icon/ios-icon-152.png" width="152" />
        <icon height="144" src="resources/ios/icon/ios-icon-144.png" width="144" />
        <icon height="120" src="resources/ios/icon/ios-icon-120.png" width="120" />
        <icon height="114" src="resources/ios/icon/ios-icon-114.png" width="114" />
        <icon height="100" src="resources/ios/icon/ios-icon-100.png" width="100" />
        <icon height="87" src="resources/ios/icon/ios-icon-87.png" width="87" />
        <icon height="80" src="resources/ios/icon/ios-icon-80.png" width="80" />
        <icon height="76" src="resources/ios/icon/ios-icon-76.png" width="76" />
